msgid "% camera"
msgstr "% fotila"

#. Page DISPLAY
#: ../src/iop/filmicrgb.c:3194 ../src/iop/filmicrgb.c:3892
msgid "display"
msgstr "ekrana"

#. axis legend
#: ../src/iop/filmicrgb.c:3203
msgid "(%)"
msgstr "(%)"

#. Page SCENE
#: ../src/iop/filmicrgb.c:3212 ../src/iop/filmicrgb.c:3728
msgid "scene"
msgstr "scena"

#. axis legend
#: ../src/iop/filmicrgb.c:3221
