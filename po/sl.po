msgstr "metoda za pridobitev profila zaslona"

#: ../build/src/preferences_gen.h:3364 ../build/src/preferences_gen.h:3389
msgctxt "preferences"
msgid "all"
msgstr "vse"

#: ../build/src/preferences_gen.h:3369
msgctxt "preferences"
msgid "xatom"
msgstr "xatom"

#: ../build/src/preferences_gen.h:3374
