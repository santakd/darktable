msgstr "Цветовые каналы при работе с масками"

#: ../build/src/preferences_gen.h:2142 ../build/src/preferences_gen.h:2165
msgctxt "preferences"
msgid "false color"
msgstr "Псевдоцвет"

#: ../build/src/preferences_gen.h:2147
msgctxt "preferences"
msgid "grey scale"
